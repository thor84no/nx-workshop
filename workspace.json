{
  "version": 2,
  "projects": {
    "api": "apps/api",
    "api-util-interface": "libs/api/util-interface",
    "store": "apps/store",
    "store-e2e": "apps/store-e2e",
    "store-feature-game-detail": "libs/store/feature-game-detail",
    "store-ui-shared": "libs/store/ui-shared",
    "store-util-formatters": "libs/store/util-formatters"
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "store"
}
